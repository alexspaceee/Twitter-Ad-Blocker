{
    "manifest_version": 3,
    "name": "Twitter Ad Blocker",
    "version": "1.0",
    "description": "A personal adblocker for Twitter on Chrome",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://twitter.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  